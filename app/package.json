{
  "name": "JobITTrending",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && rm -rf ~/.rncache && yarn install && cd android && ./gradlew clean && yarn run fix-xcode",
    "createIconFont": "./node_modules/.bin/generate-icon ./src/assets/img/customFont/styles.css --componentName=dana-queue  --prefix=icon- --fontFamily=dana-queue > ./node_modules/react-native-vector-icons/dana-queue.js && cp ./src/assets/img/customFont/dana-queue.ttf ./node_modules/react-native-vector-icons/Fonts/dana-queue.ttf",
    "buildandroid": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "fix-xcode": "react-native-schemes-manager all",
    "postinstall": "react-native-schemes-manager all && yarn run createIconFont",
    "podInstall": "pod install --repo-update"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "lottie-react-native": "^2.5.8",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.6.1",
    "react-native": "^0.57.5",
    "react-native-circular-progress": "^1.0.1",
    "react-native-code-push": "^5.4.2",
    "react-native-config": "^0.11.5",
    "react-native-device-info": "^0.24.3",
    "react-native-firebase": "^5.1.1",
    "react-native-i18n": "^2.0.15",
    "react-native-image-picker": "^0.26.10",
    "react-native-keyboard-aware-scroll-view": "^0.7.4",
    "react-native-linear-gradient": "^2.4.2",
    "react-native-navigation": "^2.1.3",
    "react-native-share": "^1.1.1",
    "react-native-svg": "^8.0.8",
    "react-native-vector-icons": "^5.0.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-persist-seamless-immutable": "^2.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "seamless-immutable": "^7.1.3",
    "immutability-helper": "2.4.0"
  },
  "xcodeSchemes": {
    "Debug": [
      "Debug"
    ],
    "Release": [
      "Staging",
      "Release"
    ],
    "projectDirectory": "iOS"
  },
  "devDependencies": {
    "ad-rn-generator": "^1.0.10",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "^0.49.1",
    "react-addons-test-utils": "^15.6.2",
    "react-native-schemes-manager": "^1.0.5",
    "react-test-renderer": "16.6.1",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
